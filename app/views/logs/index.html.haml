%h1 Listing logs
%table
  %tr
    %th created_at
    %th character
    %th srs
    %th item_type
  - @logs.each do |log|
    %tr
      %td= log.created_at
      %td.character-jp
        - if log.character.character.nil?
          %img{:src => "#{log.character.image}", :style => "filter: invert(100%)"}/
        - else
          = log.character.character
      %td= log.srs
      %td= log.character.type
      %td= link_to 'Show', log_path(log)
= will_paginate @logs
